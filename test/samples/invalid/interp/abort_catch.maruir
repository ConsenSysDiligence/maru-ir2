fun fail(x: u8): u8 {
    entry:
        abort;
}

fun middle(x: u8): u8
locals res: u8;
{
    entry:
        res := call fail(x);
        return res;
}

fun main(): u8
locals res: u8, failed: bool;
{
    entry:
        res, failed := trans_call middle(u8(1));
        branch failed succ_bb fail_bb;

    fail_bb:
        return u8(42);
    
    succ_bb:
        return res;
}
