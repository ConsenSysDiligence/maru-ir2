struct Foo {
    a: uint256;
    b: int8;
    c: bool;
}

struct Boo {
    a: bool;
}

struct[M1, M2] Goo {
    a: bool[] * M1;
    b: uint8[] * M2[] * M1[];
    c: (((int16 * M2)[]) * M1)[];
}

struct Doo {
    a: Foo * Memory;
    b: Boo<uint16> * Storage;
    c: Goo[Storage, Memory]<bool, uint8>;
}