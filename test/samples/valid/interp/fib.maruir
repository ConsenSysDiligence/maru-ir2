fun fib(n: u256): u256
locals a: u256, b: u256;
{
	entry:
		branch n == u256(0) zero other1;
	zero:
		return u256(0);
	other1:
		branch n == u256(1) one other2;
	one:
		return u256(1);
	other2:
		a := call fib(n - u256(1));
		b := call fib(n - u256(2));
		return a + b;
}

fun main()
locals t: u256;
{
	entry:
		t := call fib(u256(6));
		assert t == u256(8);
		return;
}
